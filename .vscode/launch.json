{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run iOS on device",
            "type": "cordova",
            "request": "launch",
            "platform": "ios",
            "target": "device",
            "port": 9223,
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "ionicLiveReload": false
        },
        {
            "name": "Attach to running iOS on device",
            "type": "cordova",
            "request": "attach",
            "platform": "ios",
            "target": "device",
            "port": 9220,
            "sourceMaps": true,
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "Run iOS on simulator",
            "type": "cordova",
            "request": "launch",
            "platform": "ios",
            "target": "iPhone-6s",
            "port": 9222,
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            // "devServerPort": 9099,
            "livereload": true,
            // "iosDebugProxyPort": 9221,
            // "ionicLiveReload": false,
            // "attachAttempts": 5,
            // "attachDelay": 5000
        }
    ]
}